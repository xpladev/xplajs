{
  "name": "@xpla/xpla",
  "version": "1.8.3",
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "author": "Joowon Yun <joowon@delightlabs.io>",
  "homepage": "https://github.com/xpladev/xplajs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpladev/xplajs.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./*": {
      "types": "./*.d.ts",
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./*.js": {
      "types": "./*.d.ts",
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "eslint . --fix",
    "starship:run": "sh ../../scripts/run-starship.sh",
    "starship:clean": "starship stop --config ./starship/configs/config.yaml",
    "starship:test": "vitest",
    "starship:debug": "vitest --bail"
  },
  "dependencies": {
    "@ethersproject/transactions": "^5.7.0",
    "@interchainjs/amino": "catalog:",
    "@interchainjs/auth": "catalog:",
    "@interchainjs/cosmos": "catalog:",
    "@interchainjs/cosmos-types": "catalog:",
    "@interchainjs/crypto": "catalog:",
    "@interchainjs/ethereum": "catalog:",
    "@interchainjs/types": "catalog:",
    "@interchainjs/utils": "catalog:",
    "@xpla/evm": "workspace:*",
    "@xpla/xplajs": "workspace:*",
    "@noble/hashes": "^1.3.1",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.1.0",
    "bip39": "^3.1.0",
    "decimal.js": "^10.4.3",
    "deepmerge": "4.2.2",
    "ethers": "^6.13.5",
    "interchainjs": "catalog:",
    "tsdown": "catalog:"
  },
  "keywords": [
    "xpla"
  ],
  "devDependencies": {
    "@chain-registry/types": "catalog:",
    "@interchainjs/encoding": "catalog:",
    "@xpla/evm": "^1.8.0",
    "bech32": "^2.0.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.5",
    "starshipjs": "^2.4.0",
    "vitest": "^4.0.17"
  }
}
