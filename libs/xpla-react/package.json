{
  "name": "@xpla/xpla-react",
  "version": "1.8.1",
  "description": "xpla-react is a JavaScript library providing react query hooks for interacting with xpla sdk.",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "author": "Joowon Yun <joowon@delightlabs.io>",
  "homepage": "https://github.com/xpladev/xplajs",
  "repository": {
    "type": "git",
    "url": "https://github.com/xpladev/xplajs"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./esm/index.js",
      "require": "./index.js"
    },
    "./*": {
      "types": "./*.d.ts",
      "import": "./esm/*.js",
      "require": "./*.js"
    },
    "./*.js": {
      "types": "./*.d.ts",
      "import": "./esm/*.js",
      "require": "./*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "copy": "copyfiles -f README.md package.json dist",
    "clean": "rimraf dist/**",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc --declarationMap; tsc -p tsconfig.esm.json",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@interchainjs/cosmos": "catalog:",
    "@interchainjs/cosmos-types": "catalog:",
    "@interchainjs/encoding": "catalog:",
    "@interchainjs/math": "catalog:",
    "@interchainjs/pubkey": "catalog:",
    "@interchainjs/types": "catalog:",
    "@interchainjs/utils": "catalog:",
    "@noble/hashes": "^1.3.1",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@tanstack/react-query": "4.29.1",
    "@types/node": "catalog:"
  },
  "peerDependencies": {
    "@tanstack/react-query": "4.29.1"
  },
  "keywords": [
    "cosmos",
    "blockchain",
    "transaction",
    "cosmjs",
    "wallet"
  ]
}
