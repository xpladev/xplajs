{
  "name": "@xpla/xplajs",
  "version": "1.8.2",
  "main": "index.js",
  "module": "esm/index.js",
  "author": "Joowon Yun <joowon@delightlabs.io>",
  "homepage": "https://github.com/xpladev/xplajs",
  "scripts": {
    "copy": "copyfiles -f README.md package.json dist",
    "clean": "rimraf dist/**",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run fix-esm; npm run copy",
    "fix-esm": "tsc-esm-fix --target=dist/esm"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./*.js",
      "types": "./*.d.ts"
    },
    "./*.js": {
      "import": "./esm/*.js",
      "require": "./*.js",
      "types": "./*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@cosmjs/stargate": "^0.33.0",
    "@cosmology/lcd": "^0.14.4",
    "@interchainjs/cosmos": "^1.19.2",
    "@interchainjs/cosmos-types": "^1.19.1",
    "@interchainjs/encoding": "^1.18.1",
    "@interchainjs/pubkey": "^1.19.1",
    "@interchainjs/types": "^1.19.1",
    "@interchainjs/utils": "^1.19.1",
    "@noble/hashes": "^1.3.1",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "tsc-esm-fix": "^3.1.2"
  },
  "license": "MIT",
  "description": ""
}
