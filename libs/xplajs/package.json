{
  "name": "@xpla/xplajs",
  "version": "1.8.1",
  "main": "index.js",
  "module": "esm/index.js",
  "author": "Joowon Yun <joowon@delightlabs.io>",
  "homepage": "https://github.com/xpladev/xplajs",
  "scripts": {
    "copy": "copyfiles -f README.md package.json dist",
    "clean": "rimraf dist/**",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./esm/index.js",
      "require": "./index.js"
    },
    "./*": {
      "types": "./*.d.ts",
      "import": "./esm/*.js",
      "require": "./*.js"
    },
    "./*.js": {
      "types": "./*.d.ts",
      "import": "./esm/*.js",
      "require": "./*.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@cosmjs/stargate": "catalog:",
    "@cosmjs/tendermint-rpc": "catalog:",
    "@cosmology/lcd": "^0.14.4",
    "@interchainjs/cosmos": "catalog:",
    "@interchainjs/cosmos-types": "catalog:",
    "@interchainjs/encoding": "catalog:",
    "@interchainjs/math": "catalog:",
    "@interchainjs/pubkey": "catalog:",
    "@interchainjs/types": "catalog:",
    "@interchainjs/utils": "catalog:",
    "@noble/hashes": "^1.3.1",
    "@types/node": "catalog:",
    "decimal.js": "^10.4.3"
  },
  "license": "MIT",
  "description": ""
}
