{
  "name": "@xpla/evm",
  "version": "1.8.0",
  "description": "TypeChain generated TypeScript interfaces for XPLA EVM smart contracts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    },
    "./*.js": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "xpla",
    "evm",
    "ethereum",
    "solidity",
    "typechain",
    "typescript",
    "smart-contracts",
    "blockchain"
  ],
  "author": "Joowon Yun <joowon@delightlabs.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xpladev/xplajs.git",
    "directory": "libs/evm"
  },
  "bugs": {
    "url": "https://github.com/xpladev/xplajs/issues"
  },
  "homepage": "https://github.com/xpladev/xplajs#readme",
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "catalog:",
    "tsdown": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
