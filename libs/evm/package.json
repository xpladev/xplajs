{
  "name": "@xpla/evm",
  "version": "1.8.1",
  "description": "TypeChain generated TypeScript interfaces for XPLA EVM smart contracts",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./*.js",
      "types": "./*.d.ts"
    },
    "./*.js": {
      "import": "./esm/*.js",
      "require": "./*.js",
      "types": "./*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run clean && tsc && tsc -p tsconfig.esm.json && npm run fix-esm",
    "clean": "rimraf dist",
    "fix-esm": "tsc-esm-fix --target=dist/esm",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "xpla",
    "evm",
    "ethereum",
    "solidity",
    "typechain",
    "typescript",
    "smart-contracts",
    "blockchain"
  ],
  "author": "Joowon Yun <joowon@delightlabs.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xpladev/xplajs.git",
    "directory": "libs/evm"
  },
  "bugs": {
    "url": "https://github.com/xpladev/xplajs/issues"
  },
  "homepage": "https://github.com/xpladev/xplajs#readme",
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
