{
    "compilerOptions": {
      "target": "es2022",
      "module": "esnext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictNullChecks": false,
      "skipLibCheck": true,
      "sourceMap": false,
      "declaration": true,
      "resolveJsonModule": true,
      "moduleResolution": "bundler"
    },
    "exclude": ["dist", "node_modules"]
  }
  