{
  "name": "xplajs",
  "version": "1.8.0",
  "private": true,
  "main": "index.js",
  "repository": "git@github.com:xpladev/xplajs.git",
  "scripts": {
    "proto": "telescope download --config ./scripts/xpla.protod.config.json",
    "codegen": "ts-node ./scripts/codegen.ts",
    "codegen:sol": "sh ./scripts/get-precompiles.sh",
    "codegen:evm": "hardhat typechain",
    "build": "lerna run build --stream",
    "symlink": "symlink-workspace --logLevel error",
    "prepack": "sh ./scripts/prepack.sh"
  },
  "workspaces": [
    "libs/*",
    "networks/*"
  ],
  "author": "Joowon Yun <joowon@delightlabs.io>",
  "license": "MIT",
  "description": "",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "devDependencies": {
    "@chain-registry/client": "1.18.1",
    "@chain-registry/v2": "^1.71.216",
    "@hyperweb/telescope": "^1.17.0",
    "@starship-ci/cli": "^2.9.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "copyfiles": "^2.4.1",
    "deepmerge": "4.2.2",
    "hardhat": "^2.26.2",
    "jest": "^29.6.2",
    "lerna": "^6",
    "rimraf": "4.4.1",
    "starshipjs": "^2.4.0",
    "symlink-workspace": "^1.9.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.9.2"
  }
}
